{
  "name": "legacy-challenge-resource-processor",
  "version": "1.0.0",
  "description": "Topcoder Legacy Challenge Resource Processor",
  "repository": "https://github.com/topcoder-platform/legacy-challenge-resource-processor",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:prod": "cross-env NODE_ENV=production node src/app.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_ENV=test mocha test/unit/*.test.js --require test/unit/prepare.js --timeout 30000 --exit",
    "cov": "nyc --reporter=html --reporter=text npm test",
    "e2e": "cross-env NODE_ENV=test PORT=3333 mocha test/e2e/*.test.js --timeout 60000 --exit",
    "cov-e2e": "nyc --reporter=html --reporter=text npm run e2e"
  },
  "author": "TCSCODER",
  "license": "none",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "mocha-prepare": "^0.1.0",
    "nock": "^9.4.4",
    "nyc": "^14.1.0",
    "should": "^13.2.3",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "config": "^3.0.1",
    "get-parameter-names": "^0.3.0",
    "http-status-codes": "^1.3.2",
    "joi": "^14.3.1",
    "lodash": "^4.17.10",
    "no-kafka": "^3.4.3",
    "superagent": "^4.1.0",
    "tc-core-library-js": "git+https://github.com/appirio-tech/tc-core-library-js.git#v2.6.2",
    "topcoder-healthcheck-dropin": "^1.0.2",
    "winston": "^2.2.0"
  },
  "engines": {
    "node": "10.x"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
